<div class="col-md-12">
<h1>new list</h1>
<hr>

<%= simple_form_for (@list) do |f| %>
  <div class = "form-group">
    <%= f.input :title, input_html: { class: "form-control"} %>
    <%= f.input :description, input_html: { class: "form-control"} %>
  </div>
  <%= f.submit "Submit", class: "btn-primary", data: { disable_with: "Submitting..."} %>
<% end %>
</div>
